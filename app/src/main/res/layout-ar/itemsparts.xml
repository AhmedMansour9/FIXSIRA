<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="#fff"
    android:layout_marginBottom="@dimen/_10sdp"
    android:layout_height="@dimen/_150sdp">

    <ProgressBar
        android:layout_width="@dimen/_30sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/_30sdp"
        android:id="@+id/ProgrossSpare"
        android:visibility="gone"
        />

    <ImageView
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_140sdp"
        android:layout_centerVertical="true"
        android:layout_marginRight="@dimen/_10sdp"
        android:id="@+id/img_Spare"
        android:scaleType="fitXY"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/_10sdp"
        android:layout_marginLeft="@dimen/_10sdp"
        android:id="@+id/Text_Vendor"
        android:textSize="@dimen/_14sdp"
        android:maxLines="1"
        android:textColor="#ffa200"
        android:layout_toLeftOf="@+id/img_Spare"
        android:layout_marginTop="@dimen/_20sdp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/_10sdp"
        android:layout_marginLeft="@dimen/_10sdp"
        android:id="@+id/Text_Spare"
        android:textSize="@dimen/_14sdp"
        android:layout_below="@+id/Text_Vendor"
        android:maxLines="1"
        android:textColor="#000"
        android:layout_toLeftOf="@+id/img_Spare"
        android:layout_marginTop="@dimen/_10sdp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/_10sdp"
        android:layout_marginLeft="@dimen/_10sdp"
        android:id="@+id/Text_Price"
        android:maxLines="1"
        android:layout_toLeftOf="@+id/img_Spare"
        android:textColor="#000"
        android:textSize="@dimen/_12sdp"
        android:layout_below="@+id/Text_Spare"
        android:layout_marginTop="@dimen/_10sdp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/_10sdp"
        android:layout_marginLeft="@dimen/_10sdp"
        android:id="@+id/Text_RegularPrice"
        android:maxLines="1"
        android:layout_toLeftOf="@+id/Text_Price"
        android:textColor="#000"
        android:textSize="@dimen/_12sdp"
        android:layout_below="@+id/Text_Spare"
        android:layout_marginTop="@dimen/_10sdp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#838282"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_marginRight="@dimen/_10sdp"
        android:layout_toLeftOf="@+id/img_Spare"
        android:layout_below="@+id/Text_Price"
        android:id="@+id/T_Rates"
        android:maxLines="1"
        />

    <ImageView
        android:layout_width="@dimen/_15sdp"
        android:layout_height="@dimen/_15sdp"
        android:src="@drawable/star"
        android:layout_toLeftOf="@+id/T_Rates"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_marginRight="@dimen/_15sdp"
        android:layout_below="@+id/Text_Price"
        android:id="@+id/Starone"
        android:visibility="gone"
        />
    <ImageView
        android:layout_width="@dimen/_15sdp"
        android:layout_height="@dimen/_15sdp"
        android:src="@drawable/star"
        android:layout_toLeftOf="@+id/Starone"
        android:layout_below="@+id/Text_Price"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_marginRight="@dimen/_5sdp"
        android:id="@+id/Startwo"
        android:visibility="gone"
        />
    <ImageView
        android:layout_width="@dimen/_15sdp"
        android:layout_height="@dimen/_15sdp"
        android:src="@drawable/star"
        android:layout_toLeftOf="@+id/Startwo"
        android:layout_below="@+id/Text_Price"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_marginRight="@dimen/_5sdp"
        android:id="@+id/Starthree"
        android:visibility="gone"

        />
    <ImageView
        android:layout_width="@dimen/_15sdp"
        android:layout_height="@dimen/_15sdp"
        android:src="@drawable/star"
        android:layout_toLeftOf="@+id/Starthree"
        android:layout_below="@+id/Text_Price"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_marginRight="@dimen/_5sdp"
        android:id="@+id/StarFour"
        android:visibility="gone"

        />
    <ImageView
        android:layout_width="@dimen/_15sdp"
        android:layout_height="@dimen/_15sdp"
        android:src="@drawable/star"
        android:layout_toLeftOf="@+id/StarFour"
        android:layout_below="@+id/Text_Price"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_marginRight="@dimen/_5sdp"
        android:id="@+id/StarFive"
        android:visibility="gone"
        />

    <ImageView
        android:layout_width="@dimen/_50sdp"
        android:layout_height="@dimen/_40sdp"
        android:src="@drawable/phone"
        android:id="@+id/call"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_marginBottom="@dimen/_10sdp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"

        />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_below="@+id/Text_Price"
        android:layout_toLeftOf="@+id/T_Rates"
        android:visibility="gone"
        android:id="@+id/relaa"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="@dimen/_15sdp"
            android:layout_height="@dimen/_15sdp"
            android:src="@drawable/emptystart"
            android:layout_marginTop="@dimen/_20sdp"
            android:layout_marginRight="@dimen/_15sdp"
            android:id="@+id/Staron"
            />


        <ImageView
            android:layout_width="@dimen/_15sdp"
            android:layout_height="@dimen/_15sdp"
            android:src="@drawable/emptystart"
            android:layout_toLeftOf="@+id/Staron"
            android:layout_marginTop="@dimen/_20sdp"
            android:layout_marginRight="@dimen/_5sdp"
            android:id="@+id/Startw"
            />
        <ImageView
            android:layout_width="@dimen/_15sdp"
            android:layout_height="@dimen/_15sdp"
            android:src="@drawable/emptystart"
            android:layout_toLeftOf="@+id/Startw"
            android:layout_marginTop="@dimen/_20sdp"
            android:layout_marginRight="@dimen/_5sdp"
            android:id="@+id/Starthre"

            />
        <ImageView
            android:layout_width="@dimen/_15sdp"
            android:layout_height="@dimen/_15sdp"
            android:src="@drawable/emptystart"
            android:layout_toLeftOf="@+id/Starthre"
            android:layout_marginTop="@dimen/_20sdp"
            android:layout_marginRight="@dimen/_5sdp"
            android:id="@+id/StarFou"
            />
        <ImageView
            android:layout_width="@dimen/_15sdp"
            android:layout_height="@dimen/_15sdp"
            android:src="@drawable/emptystart"
            android:layout_toLeftOf="@+id/StarFou"
            android:layout_marginTop="@dimen/_20sdp"
            android:layout_marginRight="@dimen/_5sdp"
            android:id="@+id/StarFiv"
            android:visibility="visible"
            />

    </RelativeLayout>
</RelativeLayout>