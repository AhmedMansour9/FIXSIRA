<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/profileFrame"
    android:clickable="true"
    android:background="#e1e1e1">


    <android.support.v7.widget.CardView
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_margin="20dp"
        android:padding="@dimen/_10sdp"
        app:cardBackgroundColor="@android:color/white"
        card_view:cardElevation="10dp"
        card_view:cardCornerRadius="10dp">


        <android.support.v4.widget.SwipeRefreshLayout
            android:id="@+id/swipe_Profile"
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">


                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/person_image"
                        android:layout_width="@dimen/_80sdp"
                        android:layout_height="@dimen/_60sdp"
                        android:layout_marginRight="@dimen/_15sdp"
                        android:layout_marginTop="@dimen/_25sdp"
                        android:src="@drawable/userphoto"
                        android:visibility="visible"

                        />
                    <Button
                        android:layout_width="@dimen/_100sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:background="@drawable/bgbuttons"
                        android:textSize="@dimen/_13sdp"
                        android:id="@+id/show_hours"
                        android:layout_toLeftOf="@+id/person_image"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginRight="@dimen/_20sdp"
                        android:text="@string/showtime"
                        android:textAllCaps="false"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#706a6a"
                        android:layout_below="@+id/show_hours"
                        android:layout_toLeftOf="@+id/person_image"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:layout_marginRight="@dimen/_15sdp"
                        android:id="@+id/vendorname"
                        android:textStyle="bold"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#838282"
                        android:layout_toLeftOf="@+id/person_image"
                        android:layout_marginTop="@dimen/_15sdp"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_marginRight="@dimen/_15sdp"
                        android:layout_below="@+id/vendorname"
                        android:id="@+id/vendoraddress"
                        />

                    <ImageView
                        android:layout_width="@dimen/_15sdp"
                        android:layout_height="@dimen/_15sdp"
                        android:src="@drawable/star"
                        android:layout_below="@+id/vendoraddress"
                        android:layout_toLeftOf="@+id/person_image"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginRight="@dimen/_15sdp"
                        android:id="@+id/Starone"
                        android:visibility="gone"
                        />
                    <ImageView
                        android:layout_width="@dimen/_15sdp"
                        android:layout_height="@dimen/_15sdp"
                        android:src="@drawable/star"
                        android:layout_below="@+id/vendoraddress"
                        android:layout_toLeftOf="@+id/Starone"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:id="@+id/Startwo"
                        android:visibility="gone"
                        />
                    <ImageView
                        android:layout_width="@dimen/_15sdp"
                        android:layout_height="@dimen/_15sdp"
                        android:src="@drawable/star"
                        android:layout_below="@+id/vendoraddress"
                        android:layout_toLeftOf="@+id/Startwo"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:id="@+id/Starthree"
                        android:visibility="gone"

                        />
                    <ImageView
                        android:layout_width="@dimen/_15sdp"
                        android:layout_height="@dimen/_15sdp"
                        android:src="@drawable/star"
                        android:layout_below="@+id/vendoraddress"
                        android:layout_toLeftOf="@+id/Starthree"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:id="@+id/StarFour"
                        android:visibility="gone"

                        />
                    <ImageView
                        android:layout_width="@dimen/_15sdp"
                        android:layout_height="@dimen/_15sdp"
                        android:src="@drawable/star"
                        android:layout_below="@+id/vendoraddress"
                        android:layout_toLeftOf="@+id/StarFour"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:id="@+id/StarFive"
                        android:visibility="gone"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#706a6a"
                        android:layout_below="@+id/person_image"
                        android:layout_marginTop="@dimen/_55sdp"
                        android:layout_marginRight="@dimen/_30sdp"
                        android:text="@string/Descrption"
                        android:id="@+id/diescrption"
                        android:textStyle="bold"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#706a6a"
                        android:layout_below="@+id/person_image"
                        android:layout_marginTop="@dimen/_55sdp"
                        android:maxLines="2"
                        android:layout_toLeftOf="@+id/diescrption"
                        android:layout_marginRight="@dimen/_30sdp"
                        android:id="@+id/T_discrption"
                        android:textStyle="bold"
                        />

                    <fragment
                        android:id="@+id/MMap"
                        android:name="com.google.android.gms.maps.SupportMapFragment"
                        android:layout_width="match_parent"
                        android:layout_marginLeft="@dimen/_15sdp"
                        android:layout_marginRight="@dimen/_15sdp"
                        android:layout_below="@+id/T_discrption"
                        android:layout_marginTop="@dimen/_30sdp"
                        android:background="#00000000"

                        android:layout_height="@dimen/_120sdp"/>

                    <Button
                        android:layout_width="@dimen/_110sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:background="@drawable/bgbuttons"
                        android:layout_centerHorizontal="true"
                        android:text="@string/reservee"
                        android:textSize="@dimen/_13sdp"
                        android:id="@+id/Reserves"
                        android:layout_below="@+id/MMap"
                        android:layout_marginTop="@dimen/_30sdp"
                        android:layout_marginRight="@dimen/_20sdp"
                        />
                    <Button
                        android:layout_width="@dimen/_110sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:background="@drawable/bgbuttons"
                        android:layout_alignParentLeft="true"
                        android:textSize="@dimen/_13sdp"
                        android:id="@+id/showservices"
                        android:layout_below="@+id/Reserves"
                        android:layout_marginTop="@dimen/_30sdp"
                        android:layout_marginLeft="@dimen/_20sdp"
                        android:text="@string/showservices"
                        android:textAllCaps="false"
                        android:layout_marginBottom="@dimen/_10sdp"
                        />
                    <Button
                        android:layout_width="@dimen/_110sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:background="@drawable/bgbuttons"
                        android:textSize="@dimen/_13sdp"
                        android:id="@+id/show_service"
                        android:layout_below="@+id/Reserves"
                        android:layout_marginTop="@dimen/_30sdp"
                        android:layout_marginRight="@dimen/_20sdp"
                        android:text="@string/showproducts"
                        android:textAllCaps="false"
                        android:layout_marginBottom="@dimen/_10sdp"
                        />
                </RelativeLayout>
            </ScrollView>
        </android.support.v4.widget.SwipeRefreshLayout>


    </android.support.v7.widget.CardView>

</FrameLayout>
