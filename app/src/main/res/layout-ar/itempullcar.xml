<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_margin="5dp"
    android:padding="@dimen/_10sdp"
    app:cardBackgroundColor="@android:color/white"
    card_view:cardElevation="10dp"
    card_view:cardCornerRadius="10dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:id="@+id/person_image"
            android:layout_width="@dimen/_80sdp"
            android:layout_height="@dimen/_90sdp"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/_5sdp"
            android:scaleType="fitXY"
            android:src="@drawable/userphoto"
            android:visibility="visible" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#706a6a"
            android:layout_toLeftOf="@+id/person_image"
            android:layout_marginTop="@dimen/_15sdp"
            android:layout_marginLeft="@dimen/_5sdp"
            android:id="@+id/vendorNa"
            android:text="@string/vendorname"
            android:layout_marginRight="@dimen/_10sdp"
            android:maxLines="1"
            android:textStyle="bold"
            />
        <TextView
            android:id="@+id/vendorName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_15sdp"
            android:layout_marginRight="@dimen/_5sdp"
            android:layout_toLeftOf="@+id/vendorNa"
            android:maxLines="1"
            android:textColor="#706a6a"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#706a6a"
            android:layout_toLeftOf="@+id/person_image"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_below="@+id/vendorName"
            android:layout_marginLeft="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:text="@string/placename"
            android:id="@+id/placeNa"
            android:maxLines="1"
            android:textStyle="bold"
            />

        <TextView
            android:id="@+id/placeName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/vendorName"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginRight="@dimen/_5sdp"
            android:layout_toLeftOf="@+id/placeNa"
            android:maxLines="1"
            android:textColor="#706a6a"
            android:textStyle="bold" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#706a6a"
            android:textStyle="bold"
            android:layout_toLeftOf="@+id/person_image"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginLeft="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_below="@+id/placeName"
            android:id="@+id/Addres"
            android:text="@string/address"
            android:maxLines="1"
            />

        <TextView
            android:id="@+id/Address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/placeName"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_5sdp"
            android:layout_toLeftOf="@+id/Addres"
            android:maxLines="2"
            android:textColor="#838282" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#706a6a"
            android:textStyle="bold"
            android:layout_toLeftOf="@+id/person_image"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginLeft="@dimen/_5sdp"
            android:layout_below="@+id/Address"
            android:id="@+id/price"
            android:text="@string/price"
            android:maxLines="1"
            android:layout_marginRight="@dimen/_10sdp"
            />
        <TextView
            android:id="@+id/T_Price"
            android:layout_width="@dimen/_120sdp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Address"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_5sdp"
            android:layout_toLeftOf="@+id/price"
            android:maxLines="1"
            android:textColor="#838282" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#706a6a"
            android:textStyle="bold"
            android:layout_toLeftOf="@+id/person_image"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginLeft="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_below="@+id/T_Price"
            android:id="@+id/distance"
            android:text="@string/distance"
            android:maxLines="1"
            />
        <TextView
            android:id="@+id/Distance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/T_Price"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_5sdp"
            android:layout_toLeftOf="@+id/distance"
            android:maxLines="1"
            android:textColor="#838282" />

        <TextView
            android:id="@+id/T_Rates"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Distance"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_10sdp"
            android:layout_toLeftOf="@+id/person_image"
            android:maxLines="1"
            android:textColor="#838282" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Distance"
            android:layout_toLeftOf="@+id/T_Rates">

            <ImageView
                android:id="@+id/Starone"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_15sdp"
                android:src="@drawable/star"
                android:visibility="gone" />


            <ImageView
                android:id="@+id/Startwo"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_5sdp"
                android:layout_toLeftOf="@+id/Starone"
                android:src="@drawable/star"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/Starthree"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_5sdp"
                android:layout_toLeftOf="@+id/Startwo"
                android:src="@drawable/star"
                android:visibility="gone"

                />

            <ImageView
                android:id="@+id/StarFour"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_5sdp"
                android:layout_toLeftOf="@+id/Starthree"
                android:src="@drawable/star"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/StarFive"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_5sdp"
                android:layout_toLeftOf="@+id/StarFour"
                android:src="@drawable/star"
                android:visibility="gone" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Distance"
            android:layout_toLeftOf="@+id/T_Rates"
            android:id="@+id/relaa"
            android:visibility="gone">

            <ImageView
                android:id="@+id/Staron"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_15sdp"
                android:src="@drawable/emptystart"
              />


            <ImageView
                android:id="@+id/Startw"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_5sdp"
                android:layout_toLeftOf="@+id/Staron"
                android:src="@drawable/emptystart"
                 />

            <ImageView
                android:id="@+id/Starthre"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_5sdp"
                android:layout_toLeftOf="@+id/Startw"
                android:src="@drawable/emptystart"

                />

            <ImageView
                android:id="@+id/StarFou"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_5sdp"
                android:layout_toLeftOf="@+id/Starthre"
                android:src="@drawable/emptystart"
               />

            <ImageView
                android:id="@+id/StarFiv"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_5sdp"
                android:layout_toLeftOf="@+id/StarFou"
                android:src="@drawable/emptystart"
               />

        </RelativeLayout>
    </RelativeLayout>
</android.support.v7.widget.CardView>